	NO_SUCH_ERROR,
	"(libkern/common) general/unspecified error",    /* 1 */
};

/* These codes are specified in decimal in OSReturn.h.
 */
static const char * const err_codes_libkern_metaclass[] = {
	NO_SUCH_ERROR,
	"(libkern/metaclass) runtime internal error",                                       /* 1 */
